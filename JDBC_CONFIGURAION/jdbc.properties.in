#:includeifexists &PROP_DIR;/jdbc_informix.properties

#:endif

# The testOnReserveQuery to use if one is not specified for the pool.
#:ifdef MYSQL
genericTestOnReserveQuery=SELECT 1 from DUAL
#:endif
#:ifdef MSSQL
genericTestOnReserveQuery=SELECT 1
#:endif
#:ifdef DB2
genericTestOnReserveQuery=SELECT 1 from SYSIBM.SYSDUMMY1
#:endif
#:ifdef ORACLE
genericTestOnReserveQuery=SELECT 1 from DUAL
#:endif


# Used by upgrade PostServiceInstall in GIS clump
query.upgrade.truncate.SI_VERSION.* = TRUNCATE TABLE SI_VERSION
query.upgrade.truncate.SI_VERSION.db2 = DELETE FROM SI_VERSION
query.upgrade.truncate.SI_VERSION.db2iSeries = DELETE FROM SI_VERSION
query.upgrade.truncate.SI_VERSION.db2zos = DELETE FROM SI_VERSION

query.upgrade.update.yfs.resource.permission.* = UPDATE YFS_RESOURCE_PERMISSION SET RESOURCE_KEY =? WHERE RESOURCE_KEY = (SELECT RESOURCE_KEY FROM YFS_RESOURCE where RESOURCE_ID=?)

query.upgrade.delete.yfs.resource.* = DELETE FROM YFS_RESOURCE where RESOURCE_ID =?

#this flag is for support team to use
#:includewildcard &PROP_DIR;/jdbc.properties_*_ext
openTranDebug=false

# property to check Database connection leak
detect_leaked_connections=false

# property to check statement and resultset leak
detect_leaked_statements_and_resultsets=false

 # Java getCanonicalPath can show a performance hit mounted drives.
 # Add jdbc.properties:getAbsolutePath parameter to use Java getAbsolutePath()
 # when creating documents under FS storage.
 # Default (false) uses getCanonicalPath. True uses getAbsolutePath
 getAbsolutePath = false

# Default directory to store on-disk customizations
#:ifdef CUSTOMIZATION_STORAGE_DIR
customization_storage_location=&CUSTOMIZATION_STORAGE_DIR;
#:else
customization_storage_location=&INSTALL_DIR;/customizations
#:endif
